# guess the number 


import random 

print("I want you to guess the number between a start and a stop point. Keep going.")

n = int(input("Start: "))
m = int(input("Stop: "))
x = random.randrange(n,m)
#print(x)
ans = int(input("What is your guess? "))

while ans != x:
  #print(x)
  if x<ans:
    print("To high")
    ans = int(input("Try again: "))
    continue
  elif x>ans:
    print("To low")
    ans = (int(input("Try again: "))) 
    continue
if x == ans:
  print("Wow, amazing!")
